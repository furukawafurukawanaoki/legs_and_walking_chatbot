{
    "regions": [
        "hnd1"
    ],
    "builds": [
        {
            "src": "apl/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apl/wsgi.py"
        }
    ]
}